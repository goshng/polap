# You need polap src folder.
e.g.,
ln -s polap/src

# mtDNA of Carex pseudochinensis
src/run
conda activate polap-dev
src/run-polap-mtdna-carex-pseudochinensis-01.sh
src/run-polap-mtdna-carex-pseudochinensis-02.sh 14896232
src/run-polap-mtdna-carex-pseudochinensis-03.sh 184252
src/run-polap-mtdna-carex-pseudochinensis-04.sh
src/run-polap-mtdna-carex-pseudochinensis-05.sh
src/run-polap-mtdna-carex-pseudochinensis-06.sh 143720
src/run-polap-mtdna-carex-pseudochinensis-07.sh 63 143720
src/run-polap-mtdna-carex-pseudochinensis-08.sh 143720
conda activate polap-fmlrc
src/run-polap-mtdna-carex-pseudochinensis-09.sh 
src/run-polap-mtdna-carex-pseudochinensis-10.sh 
conda deactivate

# polap
conda activate polap-dev
polap -h
polap run_output1
polap run_jellyfish
polap run_flye1 -g 150000
polap run_annotation 0
polap run_genecount 0
polap run_flye2 0 1
polap run_samtools -p mt.0.fasta
conda activate polap-fmlrc
polap run_polish1
polap run_polish2 -p pt.0.fasta -f pt.1.fa
conda deactivate



